#include <stdio.h>
#include <stdlib.h>


int main(){
    int n;
    printf("Sk Mudhaseer Ahamed-192211968\n");
    printf("Enter Number: ");
    scanf("%d", &n);
    int flag=0;

    
    

    for(int i=2; i<=n/2; i++){
        if(n%i==0){
            flag = 1;
            break;

        }
    }

    if(flag==0){
        printf("%d is a Prime Number",n);
    }

    else{
        printf("%d is not a Prime Number",n);
    }

    return 0;
}

