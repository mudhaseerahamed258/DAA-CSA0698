#include <stdio.h>

int main(){

    int n1,n2,i,gcd;

    printf("Sk Mudhaseer Ahamed-192211968\n");

    printf("Enter 1st Number: ");
    scanf("%d", &n1);

    printf("Enter 2nd Number: ");
    scanf("%d", &n2);

    for(int i=1; i<=n1 && i<=n2; i++){
        if(n1%i==0 && n2%i==0){
            gcd = i;
        }
    }


    printf("Gcd of %d and %d is %d",n1,n2,gcd);

    return 0;
}
