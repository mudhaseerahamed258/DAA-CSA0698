#include <stdio.h>

int main(){
    int n,rem,sum=0;
    printf("Sk Mudhaseer Ahamed-192211968\n");
    printf("Enter a Number:");
    scanf("%d", &n);

    int temp = n;

    while(temp>0){
        rem = temp%10;
        sum = sum+(rem*rem*rem);
        temp/=10;

    }

    if(sum==n){
        printf("%d is an Armstrong Number", n);
    
    }
    else{
        printf("%d is not an Armstrong Number", n);
    }

    return 0;
}
